BEGIN TRANSACTION;

INSERT INTO History (Username, BeachId, ViewedAt)
SELECT 'user', b.Id, datetime('now','-6 hours')
FROM Beaches b
WHERE b.Name='Ναυάγιο'
  AND NOT EXISTS (SELECT 1 FROM History h WHERE h.Username='user' AND h.BeachId=b.Id);

INSERT INTO History (Username, BeachId, ViewedAt)
SELECT 'user', b.Id, datetime('now','-5 hours')
FROM Beaches b
WHERE b.Name='Ελαφονήσι'
  AND NOT EXISTS (SELECT 1 FROM History h WHERE h.Username='user' AND h.BeachId=b.Id);

INSERT INTO History (Username, BeachId, ViewedAt)
SELECT 'demo', b.Id, datetime('now','-1 day')
FROM Beaches b
WHERE b.Name='Σαρακήνικο'
  AND NOT EXISTS (SELECT 1 FROM History h WHERE h.Username='demo' AND h.BeachId=b.Id);

COMMIT;

